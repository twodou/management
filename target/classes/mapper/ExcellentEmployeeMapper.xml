<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bhb.management.mapper.ExcellentEmployeeMapper">

    <select id="excellentEmployee" resultType="com.bhb.management.pojo.ExcellentEmployee">
        select *
        from excellent
        order by id DESC
        limit 1;
    </select>

</mapper>